public class Student extends Person {
    private String program;
    private int year;
    private double fee;

    public Student() {
    }

    public Student(String _name, String _address, String _program, int _year, double _fee) {
        super(_name, _address);

        this.program = _program;
        this.year = _year;
        this.fee = _fee;
    }

    public String getProgram() {
        return program;
    }

    public void setProgram(String _program) {
        this.program = _program;
    }

    public int getYear() {
        return year;
    }

    public void setYear(int _year) {
        this.year = _year;
    }

    public void setFee(double _fee) {
        this.fee = _fee;
    }

    public double getFee() {
        return fee;
    }

    public String toString() {
        return "Student[" + super.toString() + ",program=" + this.program + ",year=" + this.year + ",fee=" + this.fee + "]";
    }
}
